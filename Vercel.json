{    
    "builds":[
        {
            "src":"./index.js",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"/"
        }
    ],
    "crons": [
        {
          "path": "/cron.js",
          "schedule": "30 1 * * *"
        }
      ]
}